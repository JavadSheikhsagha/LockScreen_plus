<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        android:id="@+id/tran_stack_fatherooooo"
        motion:constraintSetEnd="@+id/end_stack_notifo"
        motion:constraintSetStart="@id/start_stack_notifo"
        motion:duration="800">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start_stack_notifo">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end_stack_notifo">
        <Constraint
            android:id="@+id/constraintLayout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>